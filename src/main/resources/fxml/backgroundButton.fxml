<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>

<Button mnemonicParsing="false" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <graphic>
      <ImageView fx:id="image" fitHeight="55.0" fitWidth="55.0" pickOnBounds="true" smooth="false" />
   </graphic>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</Button>
